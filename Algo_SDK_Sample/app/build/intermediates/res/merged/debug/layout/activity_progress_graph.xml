<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.neurosky.algo_sdk_sample.Progress_graph">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="2dp">

        <Button
            android:id="@+id/see_graph_btn"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:width="1dip"
            android:background="@android:color/holo_blue_dark"
            android:color="@color/common_google_signin_btn_text_dark_focused"
            android:shape="oval"
            android:stateListAnimator="@null"
            android:text="See in graph"
            android:textColor="@android:color/white"
            tools:layout_editor_absoluteX="258dp"
            tools:layout_editor_absoluteY="136dp" />

        <Button
            android:id="@+id/Save_button"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:width="1dip"
            android:background="@android:color/holo_blue_dark"
            android:color="@color/common_google_signin_btn_text_dark_focused"
            android:shape="oval"
            android:stateListAnimator="@null"
            android:text="Save"
            android:textColor="@android:color/white"

            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:id="@+id/gamma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gamma"
            android:layout_below="@+id/beta"
            android:layout_alignRight="@+id/see_graph_btn"
            android:layout_alignEnd="@+id/see_graph_btn"
            android:layout_marginTop="15dp" />

        <TextView
            android:id="@+id/beta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/gamma"
            android:layout_alignStart="@+id/gamma"
            android:layout_below="@+id/graph"
            android:text="Beta" />

        <TextView
            android:id="@+id/alpha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:text="Alpha"
            android:layout_below="@+id/gamma"
            android:layout_alignLeft="@+id/gamma"
            android:layout_alignStart="@+id/gamma" />

        <TextView
            android:id="@+id/deltha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/alpha"
            android:layout_alignStart="@+id/alpha"
            android:layout_below="@+id/alpha"
            android:layout_marginTop="17dp"
            android:text="Deltha" />

        <TextView
            android:id="@+id/theta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/deltha"
            android:layout_alignStart="@+id/deltha"
            android:layout_below="@+id/deltha"
            android:layout_marginTop="14dp"
            android:text="Theta" />

        <TextView
            android:id="@+id/get_beta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/graph"
            android:layout_marginEnd="45dp"
            android:layout_marginRight="45dp"
            android:layout_toLeftOf="@+id/Save_button"
            android:layout_toStartOf="@+id/Save_button" />

        <TextView
            android:id="@+id/get_gamma"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/gamma"
            android:layout_alignLeft="@+id/get_beta"
            android:layout_alignStart="@+id/get_beta" />

        <TextView
            android:id="@+id/get_alpha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/get_gamma"
            android:layout_alignStart="@+id/get_gamma"
            android:layout_alignTop="@+id/alpha" />

        <TextView
            android:id="@+id/get_deltha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/deltha"
            android:layout_toEndOf="@+id/get_alpha"
            android:layout_toRightOf="@+id/get_alpha" />

        <TextView
            android:id="@+id/get_theta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/theta"
            android:layout_alignBottom="@+id/theta"
            android:layout_toEndOf="@+id/gamma"
            android:layout_toRightOf="@+id/gamma" />
        <com.jjoe64.graphview.GraphView
            android:id="@+id/graph"
            android:layout_width="368dp"
            android:layout_height="200dp"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="1dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </RelativeLayout>

    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        >


    </FrameLayout>

</android.support.constraint.ConstraintLayout>
